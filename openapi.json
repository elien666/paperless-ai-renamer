{
    "openapi": "3.1.0",
    "info": {
        "title": "Paperless AI Renamer",
        "description": "API for Paperless AI Renamer Service",
        "version": "1.0.0"
    },
    "paths": {
        "/health": {
            "get": {
                "summary": "Health Check",
                "operationId": "health_check_health_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/scan": {
            "post": {
                "summary": "Trigger Scan",
                "description": "Manually trigger a scan for documents with bad titles.\nOptionally filter by date (YYYY-MM-DD) using 'newer_than'.",
                "operationId": "trigger_scan_scan_post",
                "parameters": [
                    {
                        "name": "newer_than",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "title": "Newer Than"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/index": {
            "post": {
                "summary": "Trigger Index",
                "description": "Manually trigger bulk indexing of existing documents.\nOptionally filter by date (YYYY-MM-DD) using 'older_than'.",
                "operationId": "trigger_index_index_post",
                "parameters": [
                    {
                        "name": "older_than",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "title": "Older Than"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/find-outliers": {
            "get": {
                "summary": "Find Outlier Documents",
                "description": "Identify documents that are isolated in the vector space. Returns a list of document IDs with outlier scores.",
                "operationId": "find_outliers_find_outliers_get",
                "parameters": [
                    {
                        "name": "k_neighbors",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 5,
                            "title": "K Neighbors"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 50,
                            "title": "Limit"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response with outlier list",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server Error"
                    }
                }
            }
        },
        "/process-documents": {
            "post": {
                "summary": "Process Specific Documents",
                "description": "Accept a list of document IDs and queue them for renaming.",
                "operationId": "process_documents_process_documents_post",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "document_ids": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "required": [
                                    "document_ids"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Processing started",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "500": {
                        "description": "Server error"
                    }
                }
            }
        },
        "/progress": {
            "get": {
                "summary": "Get Processing Progress",
                "description": "Returns total number of documents to process and how many have been processed.",
                "operationId": "get_progress_progress_get",
                "responses": {
                    "200": {
                        "description": "Progress info",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "total": {
                                            "type": "integer"
                                        },
                                        "processed": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/webhook": {
            "post": {
                "summary": "Handle Webhook",
                "description": "Handle incoming webhooks from Paperless.",
                "operationId": "handle_webhook_webhook_post",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            }
        }
    }
}